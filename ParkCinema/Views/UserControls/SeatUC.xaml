                <TextBlock FontSize="20" Height="30" FontWeight="DemiBold" Margin="110,-600,0,0" Text="{Binding Movie.MovieDateTime}"></TextBlock>
                <TextBlock FontSize="20" Height="30" FontWeight="DemiBold" Margin="200,-600,0,0" Text="{Binding Movie.Theater}"></TextBlock>
                <TextBlock FontSize="20" Height="30" FontWeight="DemiBold" Text="Adults: " Margin="100,-565,0,0"></TextBlock>
                <TextBlock FontSize="20" Height="30" FontWeight="DemiBold" Text="{Binding Count}" Margin="180,-565,0,0"></TextBlock>
                <Image Width="300" Height="180" Margin="0,70,0,0" Source="/Icons/ticket.png"></Image>
                <TextBlock FontSize="30" FontWeight="Bold" Margin="110,110,0,0" HorizontalAlignment="Center" Text="{Binding TotalPrice}"></TextBlock>
                <TextBlock FontSize="30" FontWeight="Bold" Margin="100,150,0,0" Text="AZN"></TextBlock>
                <Image Width="100"  Source="/Icons/nextarrow.png"  Margin="260,100,0,0"></Image>
                <Image Width="100" Source="/Icons/backarrow.png"  Margin="-60,100,0,0"></Image>
                <Button Command="{Binding NextPaymentButtonClickCommand}" Style="{StaticResource allButtons}" FontWeight="DemiBold" FontSize="20" Content="Next" Margin="280,133,0,0"></Button>
                <Button Command="{Binding BackSessionButtonClickCommand}" Style="{StaticResource allButtons}" FontWeight="DemiBold" FontSize="20" Content="Back" Margin="-30,133,0,0"></Button>
            </Canvas>
            <Canvas>
                <Canvas Visibility="{Binding PaymentVisibility}" Margin="300,500,0,0">
                    <TextBlock FontSize="20" Height="30" FontWeight="DemiBold" VerticalAlignment="Top"  Margin="0,-315,0,0" Text="{Binding Movie.MovieDate}"></TextBlock>
                    <TextBlock FontSize="20" Height="30" FontWeight="DemiBold" Margin="110,-600,0,0" Text="{Binding Movie.MovieDateTime}"></TextBlock>
                    <TextBlock FontSize="20" Height="30" FontWeight="DemiBold" Margin="200,-600,0,0" Text="{Binding Movie.Theater}"></TextBlock>
                    <TextBlock FontSize="20" FontWeight="Bold" Text="Total Amount :" Margin="30,-270,0,0"></TextBlock>
                    <TextBlock FontSize="20" Height="30" FontWeight="DemiBold" Margin="190,-510,0,0" Text="{Binding TotalPrice}"></TextBlock>
                    <TextBlock FontSize="20" Height="30" FontWeight="DemiBold" Margin="220,-510,0,0" Text="AZN"></TextBlock>
                    <TextBlock FontSize="20" Height="30" FontWeight="DemiBold" Text="Adults: " Margin="100,-560,0,0"></TextBlock>
                    <TextBlock FontSize="20" Height="30" FontWeight="DemiBold" Text="{Binding Count}" Margin="180,-560,0,0"></TextBlock>
                    <Image Width="300" Height="180" Margin="0,70,0,0" Source="/Icons/ticket.png"></Image>
                    <TextBlock FontSize="30" FontWeight="Bold" Margin="110,110,0,0" HorizontalAlignment="Center" Text="{Binding TotalPrice}"></TextBlock>
                    <TextBlock FontSize="30" FontWeight="Bold" Margin="100,150,0,0" Text="AZN"></TextBlock>
                    <Image Width="100" Source="/Icons/backarrow.png"  Margin="-60,100,0,0"></Image>
                    <TextBlock Height="30" FontSize="20" FontWeight="DemiBold" Text="Reserved seats : " Margin="0,-450,0,0"></TextBlock>
                    <TextBlock FontSize="15" FontWeight="DemiBold" Width="300" Margin="160,-370,0,0" Height="100" TextWrapping="Wrap" Text="{Binding Seat}"></TextBlock>
                    <Label Height="40" Margin="-50,-300,0,0" FontSize="25" FontWeight="DemiBold" Content="E-Mail"></Label>
                    <TextBox Text="{Binding EmailName}" Height="40" Width="250" Margin="70,-300,0,0" FontSize="18" FontWeight="DemiBold"></TextBox>
                    <Label Height="40" Margin="-50,-200,0,0" FontSize="25" FontWeight="DemiBold" Content="Password"></Label>
                    <TextBox Text="{Binding Password}" Height="40" Width="250" Margin="70,-190,0,0" FontSize="18" FontWeight="DemiBold"></TextBox>
                    <Button Height="35" Width="100" FontSize="18" Margin="70,-40,0,0" Foreground="White" Style="{StaticResource buyButton2}" Command="{Binding OrderCommand }" Content="Order"></Button>
                    <!--<CheckBox Margin="-46,-120,0,0" Height="20" FontSize="15" Content="I want to receive recent news">
                        <CheckBox.RenderTransform>
                            <ScaleTransform ScaleX="1.5" ScaleY="1.5"></ScaleTransform>
                        </CheckBox.RenderTransform>
                    </CheckBox>-->
                    <TextBlock FontSize="13" Margin="70,-120,0,0" Height="20" Text="Don't you have an account?"></TextBlock>
                    <Button Command="{Binding SignUpCommand}" Style="{StaticResource allButtons}" FontSize="15" Background="Transparent" Foreground="DarkBlue" FontWeight="Bold" Content="Sign up" Margin="230,-123,0,0" Height="25" Width="65"></Button>
                    <Button Command="{Binding BackPlacesButtonClickCommand}" Style="{StaticResource allButtons}" FontWeight="DemiBold" FontSize="20" Content="Back" Margin="-30,133,0,0"></Button>
                </Canvas>
                <Canvas Visibility="{Binding SignUpVisibility}">
                    <Label Height="40" Margin="150,200,0,0" FontSize="25" FontWeight="DemiBold" Content="Name"></Label>
                    <TextBox Text="{Binding UserName}" Height="40" Width="250" Margin="280,200,0,0" FontSize="18" FontWeight="DemiBold"></TextBox>
                    <Label Height="40" Margin="150,270,0,0" FontSize="25" FontWeight="DemiBold" Content="Surname"></Label>
                    <TextBox Text="{Binding Surname}" Height="40" Width="250" Margin="280,275,0,0" FontSize="18" FontWeight="DemiBold"></TextBox>
                    <Label Height="40" Margin="150,350,0,0" FontSize="25" FontWeight="DemiBold" Content="E-Mail"></Label>
                    <TextBox Text="{Binding EmailName}" Height="40" Width="250" Margin="280,350,0,0" FontSize="18" FontWeight="DemiBold"></TextBox>
                    <Label Height="40" Margin="150,450,0,0" FontSize="25" FontWeight="DemiBold" Content="Password"></Label>
                    <TextBox Text="{Binding Password}" Height="40" Width="250" Margin="280,450,0,0" FontSize="18" FontWeight="DemiBold"></TextBox>
                    <Button Height="35" Width="100" FontSize="18" Margin="280,530,0,0" Foreground="White" Style="{StaticResource buyButton2}" Command="{Binding SignedCommand }" Content="Sign Up"></Button>
                </Canvas>
            </Canvas>
        </Grid>
    </Border>
</UserControl>
