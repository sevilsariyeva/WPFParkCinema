                <ToggleButton Name="twelvesix"  Content="6" FontSize="8" IsEnabled="{Binding IsButtonEnabled}" VerticalContentAlignment="Top" Command="{Binding DataContext.PlaceClickCommand, ElementName=myGrid}" CommandParameter="{Binding ElementName=myGrid}"  Style="{StaticResource seatButton}" Grid.Row="11" Grid.Column="8"></ToggleButton>
                <ToggleButton Name="twelveseven"  Content="7" FontSize="8" IsEnabled="{Binding IsButtonEnabled}" VerticalContentAlignment="Top" Command="{Binding DataContext.PlaceClickCommand, ElementName=myGrid}" CommandParameter="{Binding ElementName=myGrid}"  Style="{StaticResource seatButton}" Grid.Row="11" Grid.Column="9"></ToggleButton>
                <ToggleButton Name="twelveeight"  Content="8" FontSize="8" IsEnabled="{Binding IsButtonEnabled}" VerticalContentAlignment="Top" Command="{Binding DataContext.PlaceClickCommand, ElementName=myGrid}" CommandParameter="{Binding ElementName=myGrid}"  Style="{StaticResource seatButton}" Grid.Row="11" Grid.Column="10"></ToggleButton>
                <ToggleButton Name="twelvenine"  Content="9" FontSize="8" IsEnabled="{Binding IsButtonEnabled}" VerticalContentAlignment="Top" Command="{Binding DataContext.PlaceClickCommand, ElementName=myGrid}" CommandParameter="{Binding ElementName=myGrid}"  Style="{StaticResource seatButton}" Grid.Row="11" Grid.Column="11"></ToggleButton>
                <ToggleButton Name="twelveten"  Content="10" FontSize="8" IsEnabled="{Binding IsButtonEnabled}" Margin="-3,0,0,0" VerticalContentAlignment="Top" Command="{Binding DataContext.PlaceClickCommand, ElementName=myGrid}" CommandParameter="{Binding ElementName=myGrid}"  Style="{StaticResource seatButton}" Grid.Row="11" Grid.Column="12"></ToggleButton>
                <ToggleButton Name="twelveeleven"  Content="11" FontSize="8" IsEnabled="{Binding IsButtonEnabled}" Margin="-3,0,0,0" VerticalContentAlignment="Top" Command="{Binding DataContext.PlaceClickCommand, ElementName=myGrid}" CommandParameter="{Binding ElementName=myGrid}"  Style="{StaticResource seatButton}" Grid.Row="11" Grid.Column="13"></ToggleButton>
                <ToggleButton Name="twelvetwelve"  Content="12" FontSize="8" IsEnabled="{Binding IsButtonEnabled}" Margin="-3,0,0,0" VerticalContentAlignment="Top" Command="{Binding DataContext.PlaceClickCommand, ElementName=myGrid}" CommandParameter="{Binding ElementName=myGrid}"  Style="{StaticResource seatButton}" Grid.Row="11" Grid.Column="14"></ToggleButton>
                <TextBlock Margin="-70,5,0,0" Text="Row 12" Width="40"></TextBlock>
                <TextBlock Text="Row 11" Grid.ColumnSpan="2" Margin="0,35,15,0" Grid.RowSpan="2"/>
                <TextBlock Text="Row 10" Grid.ColumnSpan="2" Margin="0,5,0,0" Grid.Row="2" Grid.RowSpan="2"/>
                <TextBlock Text="Row 9" Grid.ColumnSpan="2" Margin="0,5,0,0"  Grid.RowSpan="3" Grid.Row="3"/>
                <TextBlock Text="Row 8" Grid.ColumnSpan="2" Margin="0,5,0,0" Grid.RowSpan="3" Grid.Row="4"/>
                <TextBlock Text="Row 7" Grid.ColumnSpan="2" Margin="0,5,0,0" Grid.RowSpan="3" Grid.Row="5"/>
                <TextBlock Text="Row 6" Grid.ColumnSpan="2" Margin="0,5,0,0" Grid.RowSpan="3" Grid.Row="6"/>
                <TextBlock Text="Row 5" Grid.ColumnSpan="2" Margin="0,5,0,0" Grid.RowSpan="3" Grid.Row="7"/>
                <TextBlock Text="Row 4" Grid.ColumnSpan="2" Margin="0,5,0,0" Grid.RowSpan="3" Grid.Row="8"/>
                <TextBlock Text="Row 3" Grid.ColumnSpan="2" Margin="0,5,0,0" Grid.RowSpan="3" Grid.Row="9"/>
                <TextBlock Text="Row 2" Grid.ColumnSpan="2" Margin="0,5,0,0" Grid.RowSpan="3" Grid.Row="10"/>
                <TextBlock Text="Row 1" Margin="26,10,16,0" Grid.Row="11" Grid.ColumnSpan="3"/>
            </Grid>
            <Canvas Visibility="{Binding PlacesVisibility}" Margin="300,500,0,0">
                <TextBlock FontSize="20" Height="30" FontWeight="DemiBold" VerticalAlignment="Top"  Margin="0,-315,0,0" Text="{Binding Movie.MovieDate}"></TextBlock>
                <TextBlock FontSize="20" Height="30" FontWeight="DemiBold" Margin="110,-600,0,0" Text="{Binding Movie.MovieDateTime}"></TextBlock>
                <TextBlock FontSize="20" Height="30" FontWeight="DemiBold" Margin="200,-600,0,0" Text="{Binding Movie.Theater}"></TextBlock>
                <TextBlock FontSize="20" Height="30" FontWeight="DemiBold" Text="Adults: " Margin="100,-565,0,0"></TextBlock>
                <TextBlock FontSize="20" Height="30" FontWeight="DemiBold" Text="{Binding Count}" Margin="180,-565,0,0"></TextBlock>
                <Image Width="300" Height="180" Margin="0,70,0,0" Source="/Icons/ticket.png"></Image>
                <TextBlock FontSize="30" FontWeight="Bold" Margin="110,110,0,0" HorizontalAlignment="Center" Text="{Binding TotalPrice}"></TextBlock>
                <TextBlock FontSize="30" FontWeight="Bold" Margin="100,150,0,0" Text="AZN"></TextBlock>
                <Image Width="100"  Source="/Icons/nextarrow.png"  Margin="260,100,0,0"></Image>
                <Image Width="100" Source="/Icons/backarrow.png"  Margin="-60,100,0,0"></Image>
                <Button Command="{Binding NextPaymentButtonClickCommand}" Style="{StaticResource allButtons}" FontWeight="DemiBold" FontSize="20" Content="Next" Margin="280,133,0,0"></Button>
                <Button Command="{Binding BackSessionButtonClickCommand}" Style="{StaticResource allButtons}" FontWeight="DemiBold" FontSize="20" Content="Back" Margin="-30,133,0,0"></Button>
            </Canvas>
            <Canvas>
                <Canvas Visibility="{Binding PaymentVisibility}" Margin="300,500,0,0">
                    <TextBlock FontSize="20" Height="30" FontWeight="DemiBold" VerticalAlignment="Top"  Margin="0,-315,0,0" Text="{Binding Movie.MovieDate}"></TextBlock>
                    <TextBlock FontSize="20" Height="30" FontWeight="DemiBold" Margin="110,-600,0,0" Text="{Binding Movie.MovieDateTime}"></TextBlock>
                    <TextBlock FontSize="20" Height="30" FontWeight="DemiBold" Margin="200,-600,0,0" Text="{Binding Movie.Theater}"></TextBlock>
                    <TextBlock FontSize="20" FontWeight="Bold" Text="Total Amount :" Margin="30,-270,0,0"></TextBlock>
                    <TextBlock FontSize="20" Height="30" FontWeight="DemiBold" Margin="190,-510,0,0" Text="{Binding TotalPrice}"></TextBlock>
                    <TextBlock FontSize="20" Height="30" FontWeight="DemiBold" Margin="220,-510,0,0" Text="AZN"></TextBlock>
                    <TextBlock FontSize="20" Height="30" FontWeight="DemiBold" Text="Adults: " Margin="100,-560,0,0"></TextBlock>
                    <TextBlock FontSize="20" Height="30" FontWeight="DemiBold" Text="{Binding Count}" Margin="180,-560,0,0"></TextBlock>
                    <Image Width="300" Height="180" Margin="0,70,0,0" Source="/Icons/ticket.png"></Image>
                    <TextBlock FontSize="30" FontWeight="Bold" Margin="110,110,0,0" HorizontalAlignment="Center" Text="{Binding TotalPrice}"></TextBlock>
                    <TextBlock FontSize="30" FontWeight="Bold" Margin="100,150,0,0" Text="AZN"></TextBlock>
                    <Image Width="100" Source="/Icons/backarrow.png"  Margin="-60,100,0,0"></Image>
                    <TextBlock Height="30" FontSize="20" FontWeight="DemiBold" Text="Reserved seats : " Margin="0,-450,0,0"></TextBlock>
                    <TextBlock FontSize="15" FontWeight="DemiBold" Width="300" Margin="160,-370,0,0" Height="100" TextWrapping="Wrap" Text="{Binding Seat}"></TextBlock>
                    <Label Height="40" Margin="-50,-300,0,0" FontSize="25" FontWeight="DemiBold" Content="E-Mail"></Label>
                    <TextBox Text="{Binding EmailName}" Height="40" Width="250" Margin="70,-300,0,0" FontSize="18" FontWeight="DemiBold"></TextBox>
                    <Label Height="40" Margin="-50,-200,0,0" FontSize="25" FontWeight="DemiBold" Content="Password"></Label>
                    <TextBox Text="{Binding Password}" Height="40" Width="250" Margin="70,-190,0,0" FontSize="18" FontWeight="DemiBold"></TextBox>
                    <Button Height="35" Width="100" FontSize="18" Margin="70,-40,0,0" Foreground="White" Style="{StaticResource buyButton2}" Command="{Binding LoginCommand}" Content="Log in"></Button>
                    <TextBlock FontSize="13" Margin="70,-120,0,0" Height="20" Text="Don't you have an account?"></TextBlock>
                    <Button Command="{Binding SignUpCommand}" Style="{StaticResource allButtons}" FontSize="15" Background="Transparent" Foreground="DarkBlue" FontWeight="Bold" Content="Sign up" Margin="230,-123,0,0" Height="25" Width="65"></Button>
                    <Button Command="{Binding BackPlacesButtonClickCommand}" Style="{StaticResource allButtons}" FontWeight="DemiBold" FontSize="20" Content="Back" Margin="-30,133,0,0"></Button>
                </Canvas>
                <Canvas Visibility="{Binding SignUpVisibility}">
                    <Label Height="40" Margin="150,200,0,0" FontSize="25" FontWeight="DemiBold" Content="Name"></Label>
                    <TextBox Text="{Binding UserName}" Height="40" Width="250" Margin="280,200,0,0" FontSize="18" FontWeight="DemiBold"></TextBox>
                    <Label Height="40" Margin="150,270,0,0" FontSize="25" FontWeight="DemiBold" Content="Surname"></Label>
                    <TextBox Text="{Binding Surname}" Height="40" Width="250" Margin="280,275,0,0" FontSize="18" FontWeight="DemiBold"></TextBox>
                    <Label Height="40" Margin="150,350,0,0" FontSize="25" FontWeight="DemiBold" Content="E-Mail"></Label>
                    <TextBox Text="{Binding EmailName}" Height="40" Width="250" Margin="280,350,0,0" FontSize="18" FontWeight="DemiBold"></TextBox>
                    <Label Height="40" Margin="150,450,0,0" FontSize="25" FontWeight="DemiBold" Content="Password"></Label>
                    <TextBox Text="{Binding Password}" Height="40" Width="250" Margin="280,450,0,0" FontSize="18" FontWeight="DemiBold"></TextBox>
                    <Button Height="35" Width="100" FontSize="18" Margin="280,530,0,0" Foreground="White" Style="{StaticResource buyButton2}" Command="{Binding SignedCommand }" Content="Sign Up"></Button>
                </Canvas>
                <Canvas Visibility="{Binding CardVisibility}">
                    <Label Height="40" Margin="150,200,0,0" FontSize="25" FontWeight="DemiBold" Content="Card number(16 digits)"></Label>
                    <TextBox Text="{Binding CardNumber}" Height="40" Width="250" Margin="480,200,0,0" FontSize="18" FontWeight="DemiBold"></TextBox>
                    <Label Height="40" FontSize="25" FontWeight="DemiBold" Content="Month" Canvas.Left="202" Canvas.Top="270" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                    <TextBox Text="{Binding Month}" Height="40" Width="60" FontSize="18" FontWeight="DemiBold" Canvas.Left="302" Canvas.Top="275" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                    <Label Height="40" FontSize="25" FontWeight="DemiBold" Content="Year" Canvas.Left="370" Canvas.Top="270" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                    <TextBox Text="{Binding Year}" Height="40" Width="60" FontSize="18" FontWeight="DemiBold" Canvas.Left="440" Canvas.Top="275" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                    <Label Height="40" FontSize="25" FontWeight="DemiBold" Content="CVC" Canvas.Left="518" Canvas.Top="270" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                    <TextBox Text="{Binding CVC}" Height="40" Width="90" FontSize="18" FontWeight="DemiBold" Canvas.Left="606" Canvas.Top="275" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                    <Button Style="{StaticResource buyButton2}" Content="Order" Command="{Binding OrderCommand}" Canvas.Left="400" Canvas.Top="350" Width="100" Height="40" Foreground="White" FontWeight="Bold" FontSize="20"></Button>
                </Canvas>
                <Canvas Visibility="{Binding SuccessVisibility}">
                    <Image Canvas.Left="420" Canvas.Top="275"  Source="/Icons/check.png" />
                    <TextBlock Canvas.Left="280" Canvas.Top="450" FontSize="40" FontWeight="Bold" Text="Payment is successfull!" ></TextBlock>
                </Canvas>
            </Canvas>
        </Grid>
    </Border>
</UserControl>
